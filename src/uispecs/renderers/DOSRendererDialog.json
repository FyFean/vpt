// #part /uispecs/renderers/DOSRendererDialog

{
  "type": "panel",
  "children": [
    {
      "type": "field",
      "label": "Steps:",
      "children": [
        {
          "type": "spinner",
          "bind": "steps",
          "value": 10,
          "min": 1
        }
      ]
    },
    {
      "type": "field",
      "label": "Slices:",
      "children": [
        {
          "type": "spinner",
          "bind": "slices",
          "value": 200,
          "min": 1
        }
      ]
    },
    {
      "type": "field",
      "label": "Extinction:",
      "children": [
        {
          "type": "spinner",
          "bind": "extinction",
          "logarithmic": true,
          "value": 100,
          "min": 0,
          "step": 0.1
        }
      ]
    },
    {
      "type": "field",
      "label": "Aperture (Â°):",
      "children": [
        {
          "type": "spinner",
          "bind": "aperture",
          "value": 85,
          "min": 0,
          "max": 89,
          "step": 0.1
        }
      ]
    },
    {
      "type": "field",
      "label": "Occlusion samples:",
      "children": [
        {
          "type": "spinner",
          "bind": "samples",
          "value": 10,
          "min": 1,
          "max": 200,
          "step": 1
        }
      ]
    },
    {
      "type": "accordion",
      "label": "Transfer function",
      "children": [
        {
          "type": "panel",
          "bind": "tfcontainer"
        }
      ]
    }
  ]
}