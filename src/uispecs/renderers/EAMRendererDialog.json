// #part /uispecs/renderers/EAMRendererDialog

{
  "type": "panel",
  "children": [
    {
      "type": "field",
      "label": "Slices",
      "children": [
        {
          "type": "spinner",
          "bind": "slices",
          "value": 64,
          "min": 1
        }
      ]
    },
    {
      "type": "field",
      "label": "Extinction",
      "children": [
        {
          "type": "spinner",
          "bind": "extinction",
          "logarithmic": true,
          "value": 100,
          "min": 0,
          "step": 0.1
        }
      ]
    },
    {
      "type": "accordion",
      "label": "Transfer function",
      "children": [
        {
          "type": "panel",
          "bind": "tfcontainer"
        }
      ]
    }
  ]
}